lex load {
    use primitives
    import obj UInt8 from primitives
}

ld load() {
    obj NULL
    obj BH = boothash

    obj info = create BH
    obj subinfo = create BH
    bindkey info, "array", subinfo

    do {
        obj type = newtype NULL, "VMArray"
        bindkey subinfo, "type", UInt8
        composetype type, info
        lex ArrayUInt8 = type
    }

    obj context = ctx
    bindcurhllsym "arrays", context
}
