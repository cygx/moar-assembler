lex import {
    obj ArrayUInt8
}

lex main : import

fn main() {
    obj fh = open "lib/primitives.moarvm", "r"
    obj buf = create ArrayUInt8
    read fh, buf, 8
    close fh
    say "ok"
    exit 0
}

fn import() {
    use arrays

    obj type = getlexrel arrays, "ArrayUInt8"
    bindlex ArrayUInt8, type

    obj context = ctx
    forceouterctx main, context
    main()
}
